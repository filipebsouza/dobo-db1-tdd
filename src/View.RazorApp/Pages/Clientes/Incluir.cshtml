@model Domain.Clientes.Dtos.ClienteDto
<form asp-controller="Cliente" asp-action="Incluir" method="post">
    <div class="form-group">
        <label for="Nome">Nome</label>
        <input asp-for="Nome" type="text" name="Nome" />
    </div>
    <div class="form-group">
        <label for="SobreNome">Sobrenome</label>
        <input asp-for="SobreNome" type="text" name="SobreNome" />
    </div>
    <div class="form-group">
        <label for="DataDeNascimento">Data de Nascimento</label>
        <input asp-for="DataDeNascimento" type="text" name="DataDeNascimento" />
    </div>
    <div class="form-group">
        <label for="CPF">CPF</label>
        <input asp-for="CPF" type="text" name="CPF" />
    </div>
    <div class="form-group">
        <label for="RG">RG</label>
        <input asp-for="RG" type="text" name="RG" />
    </div>
    <button type="submit" class="btn btn-primary">Salvar</button>
    <a class="nav-link" asp-controller="Cliente" asp-action="Index" class="btn btn-secondary">Voltar</a>
</form>